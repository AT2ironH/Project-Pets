<h1 class="greetingsTwo">Here are your matches!</h1>

<div class="btn-container">
  <a href="/allpetowners" class="btn btn-primary btn-go-and-find new-btn">edit search</a>
</div>
</div>


<div class="one">
  {{#each result}}
    <div class="card" style="width: 18rem;">
      <img src="{{this.image}}" alt="">
        <div class="card-body">
        <a href="/allpetowners/{{ this._id }}"></a>
          <p class="card-title">{{this.name}} & {{this.ownerPetName}}</p>  
         <h5 class="card-text city">{{this.city}}</h5>
          <a href="/{{this._id}}/ownerdetails" class="btn btn-primary stretched-link card-btn">go to profile</a>
  {{/each}} 
</div>


<div class="one">

   <div class="card" style="width: 18rem;">
      <img class="profile-pic" src={{userData.image}}>
        <div class="card-body">
          <h5 class="text">{{userData.name}}</h5>
          <p class="text">{{userData.email}}</p>
          <p class="text">{{userData.address}}</p>
          <p class="text">{{userData.city}}</p>
          <p class="text">{{userData.country}}</p>
          <br>
          {{!-- <h3 class="text">my pet preference:</h3> --}}
          <p class="text">{{userData.guestPet}}</p>
          {{!-- <h3 class="text">about me:</h3> --}}
          <p class="text">{{userData.aboutMe}}</p>
          
        </div>
  </div>
</div> 

